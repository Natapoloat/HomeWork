#include<stdio.h>
int func1(int,int,int);
int main(){
	int x,y,z;
	printf("Enter Num1:");
	scanf("%d",&x);
	printf("Enter Num2:");
	scanf("%d",&y);
	printf("Enter Num3:");
	scanf("%d",&z);
	printf("The Highest Num is %d",func1(x,y,z));
}
int func1(int a,int b,int c){
	int max = a;
	if(b > max){
		max = b;
	}
	if(c > max){
		max = c;
	}
	return max;
}
